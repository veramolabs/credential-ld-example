    "babel-jest": "27.5.1",
    "jest": "27.5.1",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "@types/node": "^16.0.0",
    "@veramo/core": "^3.1.2-next",
    "@veramo/credential-ld": "^3.1.2-next",
